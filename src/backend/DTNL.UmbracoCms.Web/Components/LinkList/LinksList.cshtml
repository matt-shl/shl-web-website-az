@using DTNL.UmbracoCms.Web.Components
@model DTNL.UmbracoCms.Web.Components.LinkList

@if (Model.Links.Count == 0)
{
    return;
}

<div class="c-link-list @Model.CssClasses">
    <ul class="link-list__list">
        @foreach(Link link in Model.Links)
        {
            <li class="link-list__item">
                <partial name="Link"model="@link"/>
            </li>
        }
    </ul>
</div>
