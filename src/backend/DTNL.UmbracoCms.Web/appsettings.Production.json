{
  "$schema": "./appsettings-schema.json",
  "AzureKeyVault": {
    "Url": "https://kv-web-prd-sdc.vault.azure.net/"
  },
  "Application": {
    "HostingEnvironment": "AzureWebApp",
    "ServerRole": "Subscriber",
    "CrawlableDomains": [
      "shl-medical"
    ]
  },
  "ConnectionStrings": {
    "umbracoDbDSN": ""
  },
  "Umbraco": {
    "CMS": {
      // workaround for Smidge minification issue
      "RuntimeMinification": {
        "UseInMemoryCache": false,
        "Enabled": false
      },
      "Global": {
        "Smtp": {
          "From": "your@email.here",
          "Host": "localhost",
          "Port": 25
        }
      },
      "WebRouting": {
        "UmbracoApplicationUrl": "https://cms.shl-medical"
      }
    },
    "Storage": {
      "AzureBlob": {
        "Media": {
          "ConnectionString": null,
          "ContainerName": "media"
        }
      }
    }
  },
  "uSync": {
    "Sets": {
      "default": {
        "DisabledHandlers": [ "ContentHandler", "MediaHandler", "DomainHandler" ],
        "Handlers": {
          "DictionaryHandler": {
            "Settings": {
              "OneWay": true
            }
          }
        }
      }

    }
  }
}
