{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:5167",
      "sslPort": 44331
    }
  },
  "profiles": {
    "IIS Express - Development": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "umbraco",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "IIS Express - Test": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Test",
        "ASPNETCORE_DETAILEDERRORS": "1",
        "APPSETTINGS_OVERRIDE": "Debug"
      }
    },
    "Development": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "applicationUrl": "https://localhost:44331;http://localhost:5167",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Test": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "applicationUrl": "https://localhost:44331;http://localhost:5167",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Test",
        "ASPNETCORE_DETAILEDERRORS": "1",
        "APPSETTINGS_OVERRIDE": "Debug"
      }
    }
  }
}
