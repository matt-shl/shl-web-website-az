            "image": [
              "{\"Id\":\"88nmvnfptm38sz8s3z4p4fhq\",\"FileName\":\"Elexy_cassette_multiple_round_20240711.png\",\"Url\":\"https://cdn.bfldr.com/DTG6CG68/at/88nmvnfptm38sz8s3z4p4fhq/Elexy_cassette_multiple_round_20240711.png\",\"AssetId\":\"h7ngftp8bjnhspccq8mjnt\",\"AssetName\":\"Elexy Cassette Multiple Round - Molly and Maggie\",\"AssetDescription\":\"\"}"
