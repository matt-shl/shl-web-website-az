    {
      "contentTypeKey": "faa47dda-a4ae-4f0c-b1d3-43d9a49e9151",
      "udi": "umb://element/eb5911fe9b4a49d0a9eeeff246d688f7",
      "title": "Innovation comes in many forms at SHL",
      "description": "As the innovation leader and pioneer, we shape the future and reinvent drug\ndelivery solutions together with our customers.",
      "cards": {
        "layout": {
          "Umbraco.BlockList": [
            {
              "contentUdi": "umb://element/d280edb1097b4e8d8692fa034cc37858"
            },
            {
              "contentUdi": "umb://element/16b21dd2050248c0ac48b844ed1e2cf2"
