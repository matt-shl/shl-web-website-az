      "title": "Meet Molly®",
      "image": [
        "{\"Id\":\"f5qt5bfcwwxshqg8gj7xv7p\",\"FileName\":\"Molly Modular Platform Autoinjector-FHD-high.gif\",\"Url\":\"https://cdn.bfldr.com/DTG6CG68/at/f5qt5bfcwwxshqg8gj7xv7p/Molly_Modular_Platform_Autoinjector-FHD-high.gif\",\"AssetId\":\"gqqqt6wz74k3vs7gwjvn2gkm\",\"AssetName\":\"Molly Modular Platform Autoinjector\",\"AssetDescription\":\"\"}"
