            "image": [
              "{\"Id\":\"q6gm77xrpwsnbrxx9nvr7vw\",\"FileName\":\"Molding_DSC_1293.png\",\"Url\":\"https://cdn.bfldr.com/DTG6CG68/at/q6gm77xrpwsnbrxx9nvr7vw/Molding_DSC_1293.png\",\"AssetId\":\"jshjs47fgs8ckf784zxksfkp\",\"AssetName\":\"Molding_DSC_1293\",\"AssetDescription\":\"\"}"
