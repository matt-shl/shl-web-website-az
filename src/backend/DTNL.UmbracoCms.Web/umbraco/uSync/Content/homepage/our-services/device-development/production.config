            "image": [
              "{\"Id\":\"gwpngmp87tfgx4c4zw9gwv6\",\"FileName\":\"shl-medical-smart-autoinjector-molly-20240516-2.jpg\",\"Url\":\"https://cdn.bfldr.com/DTG6CG68/at/gwpngmp87tfgx4c4zw9gwv6/shl-medical-smart-autoinjector-molly-20240516-2.jpg\",\"AssetId\":\"f79m9nppnchhr52x9gvc58r\",\"AssetName\":\"SMART-Autoinjector-Molly-20240516-2\",\"AssetDescription\":\"\"}"
