            "image": [
              "{\"Id\":\"2npzfpw5t8sx2tv72mxjg3g\",\"FileName\":\"FAAM0652e_r.jpg\",\"Url\":\"https://cdn.bfldr.com/DTG6CG68/at/2npzfpw5t8sx2tv72mxjg3g/FAAM0652e_r.jpg\",\"AssetId\":\"26mt9827n72jsw4zxgsw368\",\"AssetName\":\"FAAM0652e_r\",\"AssetDescription\":\"\"}"
