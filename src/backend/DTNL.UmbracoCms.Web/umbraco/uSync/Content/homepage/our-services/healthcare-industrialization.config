            "image": [
              "{\"Id\":\"t9k3t5qmzg88kwfnkb6mc5p\",\"FileName\":\"enhanced-assembly-machine.jpg\",\"Url\":\"https://cdn.bfldr.com/DTG6CG68/at/t9k3t5qmzg88kwfnkb6mc5p/enhanced-assembly-machine.jpg\",\"AssetId\":\"fghs4mcx5n4qnbkjchwg392s\",\"AssetName\":\"enhanced-nr-edit\",\"AssetDescription\":\"\"}"
