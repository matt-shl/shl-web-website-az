            "title": "- Device Landscape",
            "text": {
              "markup": "<p><br><br></p>",
              "blocks": {
                "contentData": [],
                "settingsData": []
              }
            },
            "icon": [],
            "link": ""
          },
          {
            "contentTypeKey": "4f676562-3620-4dd8-84f6-41e3a4bb26d9",
            "udi": "umb://element/cdb7bfe851aa4038b70890340b9bf0ca",
            "title": "- Device Development",
            "text": "",
