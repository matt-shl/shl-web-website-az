        "{\"Id\":\"nmnsctpwg4s9hhjmmw6sj4\",\"FileName\":\"MicrosoftTeams-image-105.jpg\",\"Url\":\"https://cdn.bfldr.com/DTG6CG68/at/nmnsctpwg4s9hhjmmw6sj4/MicrosoftTeams-image-105.jpg\",\"AssetId\":\"7nw5mrkvtj4jbqp32z7z5q\",\"AssetName\":\"MicrosoftTeams-image-105\",\"AssetDescription\":\"\"}"
