            "udi": "umb://element/62c635f050464d11a831087ca1fde872",
            "image": [
              "{\"Id\":\"kxs42w5zxhctbhx8b3x64k\",\"FileName\":\"SHL-Medical_2024_Level_Up_Workshop_ZUG-20240808-6.jpg\",\"Url\":\"https://cdn.bfldr.com/DTG6CG68/at/kxs42w5zxhctbhx8b3x64k/SHL-Medical_2024_Level_Up_Workshop_ZUG-20240808-6.jpg\",\"AssetId\":\"xnkt4pkmr7xg2krh2889nbwq\",\"AssetName\":\"SHL-Medical 2024 Level Up Workshop ZUG-20240808-6\",\"AssetDescription\":\"\"}"
