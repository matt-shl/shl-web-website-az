            "contentTypeKey": "d2b19bb1-da31-4dac-b251-032514e2246d",
            "udi": "umb://element/c0e9d25d61324da7b02a25156436f72b",
            "title": "120+",
            "subTitle": "Million autoinjectors reach patients per year",
            "text": {
              "markup": "<p>And we continue to grow annually, expanding our reach to support more patients.</p>",
              "blocks": {
                "contentData": [],
                "settingsData": []
              }
            }
