    {
      "contentTypeKey": "fef1ced5-3e44-4c02-a3ba-ece0f3223fac",
      "udi": "umb://element/1aed0314c79440b380f775f9cd57434d",
      "title": "The hiring process",
      "text": {
        "markup": "<p>Our recruitment process is focused on allowing you to show your personality, experience, and competencies, and we aim to provide an experience that is authentic and supportive, giving a great sense of our culture and who we are.</p>",
        "blocks": {
          "contentData": [],
          "settingsData": []
        }
      },
